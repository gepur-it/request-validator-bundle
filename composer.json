{
  "name": "gepur-it/request-validator-bundle",
  "description": "Request validator for symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "version":"1.0.0",
  "authors": [
    {
      "name": "Andrii Yakovlev",
      "email": "yawa20@gmail.com"
    }
  ],
  "keywords": [
    "gepur",
    "symfony",
    "bundle",
    "request",
    "validator"
  ],
  "homepage": "https://github.com/gepur-it/request-validator-bundle",
  "require": {
    "php": "^7.2",
    "symfony/http-kernel": "^4.2",
    "symfony/console": "^4.2",
    "doctrine/annotations": "^1.6",
    "symfony/validator": "^4.2",
    "symfony/property-access": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/config": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "GepurIt\\RequestValidatorBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GepurIt\\RequestValidatorBundle\\Tests\\": "Tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
